{
  "editor.tabSize": 2,
  "typescript.tsdk": "node_modules/typescript/lib",
  "cSpell.words": [
    "Ophaned",
    "Refs",
    "memoise",
    "mutex",
    "syntaxfanatics",
    "upsert"
  ],
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[javascriptreact]": {
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.formatOnSave": false
  },
  "[typescriptreact]": {
    "editor.formatOnSave": false
  },
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    "javascript",
    {
      "autoFix": true,
      "language": "typescript"
    },
    {
      "autoFix": true,
      "language": "typescriptreact"
    }
  ],
  "prettier.eslintIntegration": true,
}