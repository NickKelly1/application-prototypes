{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "CreditCard": {
            "properties": {
                "expires_at": {
                    "type": "string"
                },
                "number": {
                    "type": "number"
                },
                "owner": {
                    "properties": {
                        "date_of_birth": {
                            "type": "string"
                        },
                        "name": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                }
            },
            "type": "object"
        },
        "Employee": {
            "properties": {
                "creditCards": {
                    "items": {
                        "type": "number"
                    },
                    "type": "array"
                },
                "email": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                },
                "previousPasswords": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                }
            },
            "type": "object"
        }
    }
}

