{
  "name": "name-goes-here",
  "version": "1.0.0",
  "main": "src/app.ts",
  "author": "author name",
  "license": "ISC",
  "description": "DESCRIPTION GOES HERE",
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.12",
    "@types/pg": "^7.11.0",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^6.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "graphqlgen": "^0.6.0-rc9",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-node-dev": "^1.0.0-pre.40"
  },
  "dependencies": {
    "dotenv": "^8.1.0",
    "eslint-plugin-jest": "^22.15.1",
    "graphql-yoga": "^1.18.2",
    "patch-package": "^6.1.2",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.3.0",
    "typeorm": "^0.2.18",
    "typescript": "^3.5.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "start": "ts-node src/app.ts",
    "dev": "ts-node-dev --inspect --notify false --prefer-ts --debug --respawn src/app.ts",
    "dev:docker:win": "ts-node-dev --poll --interval 1000 --inspect --notify false --debug --respawn --prefer-ts src/app.ts",
    "dev:inspect": "ts-node-dev --inspect --notify false --prefer-ts --debug --respawn src/app.ts",
    "dev:inspect-break": "ts-node-dev --inspect-brk --notify false --prefer-ts --debug --respawn src/app.ts",
    "generate": "graphqlgen",
    "postinstall": "patch-package",
    "test": "jest",
    "test:watch": "jest --watch",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migration:generate": "npm run typeorm -- migration:generate --config src/config/ormconfig.json --connection  --name ",
    "migration:run": "npm run typeorm -- migration:run"
  }
}